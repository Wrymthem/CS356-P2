import java.util.ArrayList;

public class NewsFeed {
	
	private ArrayList <Observer> observers;
	
	NewsFeed () {
		observers = new ArrayList <Observer> ();
	}
	
	public void addObserver(Observer o) {
		observers.add(o);
		
	}

	public void notifyObserver() {
		for (Observer o : observers)
			o.update();
	}
	
}
